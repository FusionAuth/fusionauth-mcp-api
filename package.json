{
  "name": "fusionauth-mcp-api",
  "version": "0.1.0",
  "description": "Preview release of FusionAuth API MCP server",
  "main": "index.js",
  "scripts": {
    "build:openapi-mcp-generator": "npm run build --workspace=openapi-mcp-generator",
    "build:mcp-api": "npm run build --workspace=mcp-api",
    "generate": "cd packages/openapi-mcp-generator && bin/openapi-mcp-generator.js --input https://raw.githubusercontent.com/FusionAuth/fusionauth-openapi/refs/heads/main/openapi.yaml --output ../mcp-api --force",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "fusionauth",
    "mcp",
    "api"
  ],
  "author": "Brent Halsey",
  "license": "Apache-2.0",
  "workspaces": [
    "packages/mcp-api",
    "packages/openapi-mcp-generator"
  ]
}
